ng'\nimport {useParams} from 'react-router-dom'\nimport {fetchOneDevices} from \"../http/deviceAPI\";\nconst DevicePage = () => {\n    const [device, setDevice] = useState({info:[]})\n    const {id} = useParams()\n    useEffect(()=>{\n        fetchOneDevices(id).then(data => setDevice(data))\n    },[])\n\n    return (\n        <Container className='mt-3'>\n            <Row className=\"flex-d justify-content-between\">\n                <Col md={4}>\n                    <Image width={300} height={300} src={'http://localhost:8080/' +  device.img}/>\n                </Col>\n                <Col md={4}>\n                    <Row  className=\"d-flex flex-column align-items-center\">\n                        <h2>{device.name}</h2>\n                        <div className=\"d-flex align-items-center justify-content-center\"\n                             style={{background:`url(${star}) no-repeat center center`, width:240, height:240, backgroundSize: 'color', fontSize:63}}>\n                            {device.rating}\n                        </div>\n                    </Row>\n                </Col>\n                <Col md={4}>\n                    <Card className='d-flex flex-column align-items-center justify-content-around'\n                    style={{width:300, height:300,fontSize:32, border:'5px solid lightgray'}}>\n                        <h3>Starting at: ${device.price}</h3>\n                        <Button variant={'outline-dark'}>Add to the Basket</Button>\n                    </Card>\n                </Col>\n            </Row>\n            <Row className='d-flex flex-column m-3'>\n                <h1>Specifications</h1>\n                {device.info.map((info, index) =>\n                <Row key = {info.id} style={{background:index % 2 === 0 ? 'Lightgray' : 'transparent', padding:10}}>\n                    {info.title}: {info.description}\n                </Row>\n                )}\n            </Row>\n        </Container>\n    );\n};\n\nexport default DevicePage;","/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/pages/Shop.js",["130"],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/pages/Auth.js",["131"],[],"import React, {useContext, useState} from 'react';\nimport {Button, Card, Container, Form, Row} from \"react-bootstrap\";\nimport {NavLink, useLocation, useNavigate} from \"react-router-dom\";\nimport {LOGIN_ROUTE, REGISTRATION_ROUTE, SHOP_ROUTE} from \"../utils/consts\";\nimport {login, registration} from \"../http/userApi\";\nimport {observer} from \"mobx-react-lite\";\nimport {Context} from \"../index\";\n\nconst Auth = observer(() => {\n    const location = useLocation()\n    const {user} = useContext(Context)\n    const isLogin = location.pathname === LOGIN_ROUTE\n\n    const [email, setEmail] = useState('')\n    const [password, setPassword] = useState('')\n    const navigate = useNavigate()\n\n    const click = async () => {\n        try {\n            let data;\n            if (isLogin){\n                data = await login(email, password)\n            }\n            else{\n                data = await registration(email, password)\n            }\n            user.setUser(user)\n            user.setIsAuth(true)\n            navigate(SHOP_ROUTE)\n        }\n        catch(e){\n            alert(e.response.data.message)\n        }\n    }\n\n\n    return (\n        <Container\n            className=\"d-flex justify-content-center align-items-center\"\n            style={{height:window.innerHeight-54}}\n        >\n            <Card style={{width:600}} className=\"p-5\">\n                <h2 className=\"m-auto\"> {isLogin ? 'Authorization' : 'Registration'}</h2>\n            <Form className=\"d-flex flex-column align-items-center\">\n                <Form.Control\n                    className=\"mt-3\"\n                placeholder=\"Enter your email address\"\n                    value={email}\n                    onChange={(e) => setEmail(e.target.value)}\n                    />\n                <Form.Control\n                    className=\"mt-3\"\n                    placeholder=\"Enter your password\"\n                    value={password}\n                    onChange={(e) => setPassword(e.target.value)}\n                    type='password'\n                />\n                <Row className=\"d-flex justify-content-between mt-3 pl-3 pr-3\">\n                        {isLogin ? <div className=\"pl-3 mt-4\">\n                        Not a user? <NavLink to={REGISTRATION_ROUTE}>Sign up</NavLink>\n                    </div> : <div className=\"pl-3 mt-4\">\n                            Already have an account? <NavLink to={LOGIN_ROUTE}>Sign in</NavLink>\n                        </div>}\n                    <Button variant=\"outline-success\" className=\"mt-3 align-self-end m-3\"\n                            onClick={click}\n                    >\n                        {isLogin ? \"Log In\" : \"Register\"}\n                    </Button>\n                </Row>\n            </Form>\n            </Card>\n            </Container>\n    );\n});\n\nexport default Auth;","/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/pages/Admin.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/components/AppRouter.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/store/UserStore.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/store/DeviceStore.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/components/NavBar.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/components/TypeBar.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/components/BrandBar.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/components/DeviceList.js",[],[],"/Users/prosto_evrey/WebstormProjects/online-store-PERN/client/src/components/DeviceItem.js",[],[],"/Users/prosto_evrey/WebstormPro